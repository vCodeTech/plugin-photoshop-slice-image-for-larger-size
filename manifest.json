{
  "id": "com.slicetool.pro",
  "name": "Slice Tool Pro",
  "version": "1.0.0",
  "main": "index.html",
  "controller": "script.js",
  "manifestVersion": 5,
  "host": {
    "app": "PS",
    "minVersion": "23.0.0"
  },
  "entrypoints": [
    {
      "type": "panel",
      "id": "slicetool",
      "label": {
        "default": "Slice Tool Pro"
      },
      "minimumSize": {
        "width": 320,
        "height": 500
      },
      "preferredDockedSize": {
        "width": 350,
        "height": 700
      }
    }
  ],
  "requiredPermissions": {
    "webview": {},
    "launchProcess": {},
    "allowCodeGenerationFromStrings": true,
    "localFileSystem": "fullAccess"
  },
  "icons": [
    {
      "width": 23,
      "height": 23,
      "path": "icons/icon.png",
      "scale": 1,
      "theme": "all"
    },
    {
      "width": 46,
      "height": 46,
      "path": "icons/icon@2x.png",
      "scale": 2,
      "theme": "all"
    }
  ]
}