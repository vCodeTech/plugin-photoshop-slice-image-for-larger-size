{
  "id": "com.slicetool.pro",
  "name": "Slice Tool Pro",
  "version": "1.0.0",
  "main": "index.html",
  "controller": "script.js",
  "manifestVersion": 5,
  "host": {
    "app": "PS",
    "minVersion": "23.0.0"
  },
  "entrypoints": [
    {
      "type": "panel",
      "id": "slicetool",
      "label": {
        "default": "Slice Tool Pro"
      },
      "minimumSize": {
        "width": 350,
        "height": 800
      },
      "preferredDockedSize": {
        "width": 400,
        "height": 800
      },
      "icons": [
        {
          "width": 23,
          "height": 23,
          "path": "icons/light@1x.png",
          "scale": [
            1
          ],
          "theme": [
            "darkest",
            "dark",
            "medium"
          ]
        },
        {
          "width": 23,
          "height": 23,
          "path": "icons/dark@1x.png",
          "scale": [
            1
          ],
          "theme": [
            "lightest",
            "light"
          ]
        },
        {
          "width": 46,
          "height": 46,
          "path": "icons/light@2x.png",
          "scale": [
            2
          ],
          "theme": [
            "darkest",
            "dark",
            "medium"
          ]
        },
        {
          "width": 46,
          "height": 46,
          "path": "icons/dark@2x.png",
          "scale": [
            2
          ],
          "theme": [
            "lightest",
            "light"
          ]
        }
      ]
    }
  ],
  "requiredPermissions": {
    "webview": {},
    "launchProcess": {},
    "allowCodeGenerationFromStrings": true,
    "localFileSystem": "fullAccess"
  },
  "icons": [
    {
      "width": 23,
      "height": 23,
      "path": "icons/dark@1x.png",
      "scale": 1,
      "theme": "all"
    },
    {
      "width": 46,
      "height": 46,
      "path": "icons/dark@2x.png",
      "scale": 2,
      "theme": "all"
    }
  ]
}